## Generated by Chef
# <%= @res.comment %>

[connection]
id=<%= @res.name %>
uuid=<%= @res.uuid %>
type=vpn
autoconnect=false

[vpn]
service-type=org.freedesktop.NetworkManager.vpnc
NAT Traversal Mode=natt
ipsec-secret-type=save
IPSec secret-flags=0
xauth-password-type=ask
Vendor=<%= @res.vendor %>
Xauth username=<%= @res.user_name %>
IPSec gateway=<%= @res.gateway %>
Xauth password-flags=3
IPSec ID=<%= @res.group_name %>
Perfect Forward Secrecy=server
IKE DH Group=dh2

[vpn-secrets]
IPSec secret=<%= @res.group_password %>

[ipv4]
method=auto
